{
    "name": "soot-ir",
    "displayName": "Soot IR",
    "description": "Support for Soot IR syntax",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "soot",
            "aliases": ["Soot IR", "Jimple","Grimple","Dava","Jasmin"],
            "extensions": [".jimple",".grimple",".dava",".jasmin"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "soot",
            "scopeName": "source.jimple",
            "path": "./syntaxes/jimple.tmLanguage.json"
        }]
    }
}